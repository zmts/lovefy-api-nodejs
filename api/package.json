{
  "name": "portal-ng1-expressjs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.0",
    "body-parser": "~1.13.2",
    "celebrate": "^4.0.1",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "fs-extra": "^3.0.1",
    "jimp": "^0.2.27",
    "joi": "^10.4.1",
    "jsonwebtoken": "^7.3.0",
    "knex": "^0.12.6",
    "lodash": "^4.16.4",
    "moment": "^2.15.2",
    "morgan": "~1.6.1",
    "multer": "^1.3.0",
    "mz": "^2.6.0",
    "objection": "^0.7.5",
    "pg": "^6.1.3",
    "serve-favicon": "~2.3.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "eslint": "^3.17.1",
    "standard": "^8.6.0"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "commonjs": true,
    "root": true,
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "indent": [
        "error",
        4,
        {
          "SwitchCase": 1
        }
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": "error",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "key-spacing": [
        "error",
        {
          "afterColon": true
        }
      ],
      "no-multi-spaces": "error"
    }
  }
}
